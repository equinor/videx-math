{
  "name": "@equinor/videx-math",
  "version": "1.1.3",
  "description": "A library with math used by Videx.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src --color",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src",
    "postdocs": "node postdocs.copyfiles.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/videx-math.git"
  },
  "keywords": [
    "math"
  ],
  "author": "Tom Kristian Tjemsland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/videx-math/issues"
  },
  "homepage": "https://github.com/equinor/videx-math#readme",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^30.1.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.7.0",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.26.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/test/.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
