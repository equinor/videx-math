{
  "name": "@equinor/videx-math",
  "version": "1.1.5",
  "description": "A library with math used by Videx.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "engines": {
    "npm": "^11.7.0"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src --color",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src",
    "postdocs": "node postdocs.copyfiles.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/videx-math.git"
  },
  "keywords": [
    "math"
  ],
  "author": "Tom Kristian Tjemsland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/videx-math/issues"
  },
  "homepage": "https://github.com/equinor/videx-math#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "rimraf": "^6.1.2",
    "rollup": "^3.29.5",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.26.6",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.50.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/test/.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
